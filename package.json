{
  "name": "gui",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://127.0.0.1:3000/",
  "dependencies": {
    "@hookform/error-message": "0.0.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.8.4",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "history": "^4.10.1",
    "jwt-decode": "^3.1.2",
    "mdbreact": "^4.27.0",
    "raygun4js": "^2.21.0",
    "react-bootstrap": "^1.4.0",
    "react-confirm-alert": "^2.6.2",
    "react-hook-form": "^6.11.3",
    "react-native": "^0.63.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "web-vitals": "^0.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "copy-db-local": "pg_dump uniquecoverlettergenerator > database/localDB ",
    "copy-heroku-db": "heroku pg:backups:capture && heroku pg:backups:download > database/latest.dump && rm latest.dump",
    "copy-both-db": "npm run copy-db-local && npm run copy-heroku-db",
    "drop-db": " dropdb uniquecoverlettergenerator && heroku pg:pull postgresql-shallow-57562 uniquecoverlettergenerator",
    "prep-db": "npm run copy-db-local  && npm run copy-heroku-db && npm run drop-db",
    "build": "react-scripts build && git add build && git add build && git commit -m 'run build' && git push",
    "simple-build": "react-scripts build",
    "heroku-deploy": "npm run copy-both-db || npm run build && git add build && git add build && git commit -m 'run build' && git push && git push heroku main",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  }
}
